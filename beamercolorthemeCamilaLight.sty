\mode<presentation>

% Requirement
\RequirePackage{tikz}

\definecolor{UniWhite}{RGB}{242,233,234}
\definecolor{OliveGreen}{RGB}{85,107,47}
\definecolor{CatalinaBlue}{RGB}{6,42,120}
\definecolor{IceBlue}{RGB}{200,233,233}
\definecolor{NavyBlue}{RGB}{32,42,68}
% Settings
\setbeamercolor*{title}{fg=UniWhite}
\setbeamercolor*{author}{fg=UniWhite}
\setbeamercolor*{date}{fg=UniWhite}

% Structure
\setbeamercolor{structure}{fg=NavyBlue}
\setbeamercolor{normal text}{fg=NavyBlue}
\setbeamercolor{separator}{fg=UniWhite, bg=UniWhite}
\setbeamercolor{footline}{bg=IceBlue}

\setbeamercolor*{frametitle}{fg=CatalinaBlue}

\mode
<all>