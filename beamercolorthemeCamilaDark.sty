% Settings
\setbeamercolor*{title}{fg=BrightYellow}
\setbeamercolor*{author}{fg=BrightYellow}
\setbeamercolor*{date}{fg=BrightYellow}

%Slide Background
\setbeamercolor{background canvas}{bg=black}

% Structure
\setbeamercolor{structure}{fg=RedOrange}
\setbeamercolor{normal text}{fg=BrightYellow}
\setbeamercolor{separator}{fg=UniWhite, bg=UniWhite}
\setbeamercolor*{footline}{bg=IceBlue}
\setbeamercolor*{frametitle}{fg=FluorescentGreen}

% Blocs
\setbeamercolor{block body}{bg= DarkSteelGrey, fg = Cream}
\setbeamercolor{block title}{bg= AshGrey, fg=black}

\setbeamercolor*{bibliography entry title}{fg=FluorescentGreen}
\setbeamercolor*{bibliography entry location}{fg=FluorescentGreen}
\setbeamercolor*{bibliography entry note}{fg=FluorescentGreen}
\setbeamertemplate{bibliography item}{}
